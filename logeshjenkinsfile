pipeline{
    agent any
    environment{
        PATH = "/opt/maven3/bin:$PATH"
    }
    stages{
        stage("Git checkout"){
            steps{
              git credentialsId: 'githubid', url: 'https://github.com/logeshSLK/myweb'
            }
        }    
        stage("Maven"){
            steps{
              sh "mvn clean package"
            }
        
        }
    }
}
